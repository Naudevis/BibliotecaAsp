@using Biblioteca.Business.Interfaces
@using Biblioteca.Data.Models
@inject IJSRuntime JSRuntime
@inject IBookRepository BookRepository
@inject NavigationManager Navigation


@if (Books == null)
{
    <p>Cargando libros...</p>
}
else if (Books.Count == 0)
{
    <p>No se encontraron libros.</p>
}
else
{
    <div class="row row-cols-1 row-cols-md-4 g-4">
        @foreach (var book in Books)
        {
            <div class="col">
                <div class="card ">
                    <div>

                        <button  style="top:4px;right:4px ;position:absolute" @onclick="()=>DelateCliente(book)" title="Eliminar Libro" type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="text-center" style="width:100%;margin-top:10px;">
                        <img style="width:60%" src="data:image/jpeg;base64," alt="...">

                    </div>
                    <div title="Doble clic para editar" class="card-body " @ondblclick="() => OnDoubleClick(book)">
                        <h5 class="card-title">@book.Title</h5>
                        <p class="form-label">Autor: @book.Author.Name</p>

                        <p class="form-label">Precio: ₡@book.Price</p>

                    </div>

                </div>
            </div>
        }
    </div>
}

@code {
    [Parameter] public List<Book> Books { get; set; }
    [Parameter] public RenderFragment ChildBookContent { get; set; }

    [Parameter]
    public EventCallback<Book> newBook { get; set; }


    private async Task OnDoubleClick(Book item)
    {
        try
        {
            await newBook.InvokeAsync(item);

        }
        catch (Exception e)
        {
            Console.WriteLine($"Error: {e.Message}");

        }

    }

    private async Task DelateCliente(Book book)
    {

        await BookRepository.DeleteBookAsync(book);
        Navigation.NavigateTo("/list-book", true);

    }
}
